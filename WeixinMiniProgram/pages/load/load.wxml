<view wx:if="{{show_skip_button}}" class="count_botton fade_in" bindtap="switchTab" >
    跳过{{count}}s
</view>
<view wx:elif="{{show_login_button}}">
        <button class='login_botton' type='primary' lang="zh_CN" catchtap="bindGetUserInfo">
            授权登录
        </button>
</view>
<view wx:elif="{{show_load_button}}">
        <button class='load_botton' type='primary' lang="zh_CN" catchtap="loadToApplet">
            重新加载
        </button>
</view>
